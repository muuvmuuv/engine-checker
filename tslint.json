{
  "defaultSeverity": "warning",
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "prettier": true
  },
  "rules": {
    "prettier": true,
    "ordered-imports": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-implicit-dependencies": false,
    "variable-name": false
  }
}