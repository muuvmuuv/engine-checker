{
  "name": "npm-supervisor",
  "version": "0.1.0",
  "description": "Checks package.json engines to match with global or local binaries.",
  "keywords": [
    "package",
    "check",
    "engines",
    "npm"
  ],
  "homepage": "https://github.com/muuvmuuv/npm-supervisor",
  "repository": {
    "type": "git",
    "url": "https://github.com/muuvmuuv/npm-supervisor.git"
  },
  "license": "MIT",
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "bin": {
    "supervisor": "bin/cli.js"
  },
  "types": "bin",
  "scripts": {
    "run": "node bin/cli",
    "lint": "tslint --project .",
    "test": "ava",
    "release": "np",
    "build": "del-cli dist && tsc",
    "minify": "minify dist/*.js --out-dir dist/",
    "prepublishOnly": "npm run build",
    "pretty": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "execa": "^1.0.0",
    "listr": "^0.14.3",
    "meow": "^5.0.0",
    "read-pkg": "^5.1.1",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/listr": "^0.14.0",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.0.2",
    "@types/read-pkg": "^4.0.0",
    "@types/semver": "^6.0.0",
    "ava": "^1.4.1",
    "babel-minify": "^0.5.0",
    "del-cli": "^1.1.0",
    "np": "^5.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "ava": {
    "concurrency": 4,
    "timeout": "1m",
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}
